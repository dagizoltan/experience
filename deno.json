{
  "tasks": {
    "start": "deno run --allow-all --unstable-kv src/adapters/web/server.tsx",
    "dev": "deno run --allow-all --unstable-kv --watch src/adapters/web/server.tsx",
    "test": "deno test --allow-all --unstable-kv tests/",
    "harvest": "deno run --allow-all --unstable-kv scripts/harvest_overpass.js",
    "seed": "deno run --allow-all --unstable-kv scripts/seed_europe.js"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "unstable": ["kv"],
  "imports": {
    "preact": "npm:preact@10.19.3",
    "preact-render-to-string": "npm:preact-render-to-string@6.3.1",
    "hono": "npm:hono@3.12.0",
    "ngeohash": "npm:ngeohash@0.6.3",
    "@turf/center": "npm:@turf/center@6.5.0",
    "@turf/helpers": "npm:@turf/helpers@6.5.0",
    "ulid": "npm:ulid@2.3.0"
  }
}
